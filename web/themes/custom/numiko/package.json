{
  "name": "numiko-frontend-framework",
  "version": "4.1.0",
  "description": "Numiko frontend framework",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "webpack --config webpack.config.js --watch",
    "serve": "webpack-dev-server --config webpack.config.js",
    "build": "npm run clean && webpack --config webpack.config.js",
    "build:production": "npm run clean && NODE_ENV=production webpack --config webpack.config.js",
    "bundle-analyser": "webpack-bundle-analyzer --port 4200 dist/stats.json",
    "clean": "rimraf ./dist/*"
  },
  "author": "Adam Robinson",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "accoutrement-color": "^2.3.1",
    "accoutrement-layout": "^3.0.3",
    "accoutrement-type": "^4.0.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-lodash": "^3.3.4",
    "browser-sync": "^2.26.7",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "expose-loader": "^0.7.5",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "include-media": "^1.4.9",
    "mini-css-extract-plugin": "^0.6.0",
